{
    "family": "simpleton-createsuperuser",
    "containerDefinitions": [
        {
            "name": "simpleton_migrate",
            "image": "jtpaasch/simpleton:latest",
            "memory": 100,
            "entryPoint": [
                "python",
                "manage.py",
                "newsuperuser",
                "admin",
                "admin@noaddress.com",
                "admin"
            ],
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "environment": [
                {
                    "name": "DJANGO_DB_ENGINE",
                    "value": "mysql"
                },
                {
                    "name": "DJANGO_DB_NAME",
                    "value": "testdb"
                },
                {
                    "name": "DJANGO_DB_USER",
                    "value": "master"
                },
                {
                    "name": "DJANGO_DB_PASSWORD",
                    "value": "masterpass"
                },
                {
                    "name": "DJANGO_DB_HOST",
                    "value": "my-db.cjihtdv6m0sy.us-east-1.rds.amazonaws.com"
                },
                {
                    "name": "DJANGO_DB_PORT",
                    "value": "3306"
                }
            ]
        }
    ],
    "volumes": []
}
